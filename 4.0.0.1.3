<!DOCTYPE html>
<html>
<head>
    <style>
        body {
            margin: 0;
            min-height: 100vh;
            display: flex;
            justify-content: center;
            align-items: center;
            background: #000;
            overflow: hidden;
        }

        .console {
            position: relative;
            width: 90vw;
            height: 80vh;
            background: #0a0a0a;
            border: 2px solid #00ff00;
            box-shadow: 0 0 20px rgba(0, 255, 0, 0.3);
            font-family: 'Courier New', monospace;
            color: #00ff00;
            font-size: 1em;
            padding: 20px;
            overflow: hidden;
            animation: bootUp 1s ease-in forwards,
                      glitch 0.1s 5s infinite alternate,
                      suckIn 1.5s 6s ease-out forwards;
        }

        .console-content {
            position: absolute;
            top: 20px;
            left: 20px;
            width: calc(100% - 40px);
            height: calc(100% - 40px);
            display: flex;
            flex-direction: column;
            white-space: pre-wrap;
            line-height: 1.4;
        }

        .line {
            opacity: 0;
            animation: typeLine 0.1s forwards;
        }

        /* Staggered delays for each line */
        .line:nth-child(1) { animation-delay: 1s; }
        .line:nth-child(2) { animation-delay: 1.2s; }
        .line:nth-child(3) { animation-delay: 1.4s; }
        .line:nth-child(4) { animation-delay: 1.6s; }
        .line:nth-child(5) { animation-delay: 1.8s; }
        .line:nth-child(6) { animation-delay: 2s; }
        .line:nth-child(7) { animation-delay: 2.22s; }
        .line:nth-child(8) { animation-delay: 2.24s; }
        .line:nth-child(9) { animation-delay: 2.26s; }
        .line:nth-child(10) { animation-delay: 2.28s; }
        .line:nth-child(11) { animation-delay: 2.3s; }
        .line:nth-child(12) { animation-delay: 2.32s; }
        .line:nth-child(13) { animation-delay: 2.34s; }
        .line:nth-child(14) { animation-delay: 2.36s; }
        .line:nth-child(15) { animation-delay: 2.38s; }
        .line:nth-child(16) { animation-delay: 3.4s; }
        .line:nth-child(17) { animation-delay: 3.42s; }
        .line:nth-child(18) { animation-delay: 3.44s; }
        .line:nth-child(19) { animation-delay: 3.46s; }
        .line:nth-child(20) { animation-delay: 3.48s; }
         .line:nth-child(21) { animation-delay: 3.5s; }

        .black-hole {
            position: absolute;
            width: 20px;
            height: 20px;
            background: radial-gradient(circle, #000 10%, #00ff00 50%, transparent 70%);
            border-radius: 50%;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%);
            animation: pulse 0.5s 5s infinite alternate,
                      growHole 1.5s 6s ease-out forwards;
            z-index: 1;
            box-shadow: 0 0 20px rgba(0, 255, 0, 0.5);
        }

        @keyframes bootUp {
            0% {
                opacity: 0;
                transform: scale(0.95);
            }
            100% {
                opacity: 1;
                transform: scale(1);
            }
        }

        @keyframes typeLine {
            0% {
                opacity: 0;
                transform: translateX(-10px);
            }
            100% {
                opacity: 1;
                transform: translateX(0);
            }
        }

        @keyframes glitch {
            0% {
                transform: translateX(-2px) skewX(2deg);
            }
            100% {
                transform: translateX(2px) skewX(-2deg);
            }
        }

        @keyframes suckIn {
            0% {
                transform: scale(1) rotate(0deg);
                opacity: 1;
            }
            50% {
                transform: scale(1.1) rotate(90deg);
                opacity: 0.7;
            }
            100% {
                transform: scale(0) rotate(360deg);
                opacity: 0;
            }
        }

        @keyframes growHole {
            0% {
                transform: translate(-50%, -50%) scale(0);
                opacity: 0;
            }
            20% {
                opacity: 1;
            }
            100% {
                transform: translate(-50%, -50%) scale(50);
                opacity: 1;
            }
        }

        @keyframes pulse {
            0% { box-shadow: 0 0 20px rgba(0, 255, 0, 0.5); }
            100% { box-shadow: 0 0 40px rgba(0, 255, 0, 0.8); }
        }
    </style>
</head>
